        "# 12) Reset this example\n",
        "oea.delete_dataset('contoso/v0.1')\n",
        "oea.drop_lake_db('ldb_sam_s2i_contoso_v0p1')\n",
        "oea.drop_lake_db('ldb_sam_s2r_contoso_v0p1')"
